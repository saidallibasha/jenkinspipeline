pipeline {
    agent any
        environment {
            name1 = "Vinni"
            name2 = "pavani"
        }

        stages {
            stage('Actress'){
                environment{
                    name3 = "yamini bhaskar"
                }

                steps {
                    echo "cute ${name1}"
                    echo "hot ${name2}"
                    echo "beautiful ${name3}"
                }
            }
        }
}